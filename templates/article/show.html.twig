{% extends "base.html.twig" %}
{% block title %}
    {{article.title}}
{% endblock %}
{% block body %}
    <div class="center-div">
        <h1> {{article.title}} </h1>
        <small> {{article.createdAt|date("d/m/Y")}} </small>
        <figure>
            <img src="{{ article.image }}" alt="{{ article.image }}">
        </figure>
        <p> {{ article.description }} </p>
    </div>
{% endblock %}